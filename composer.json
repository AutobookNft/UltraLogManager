{
    "name": "fabiocherici/ultra-log-manager",
    "description": "Ultra Log Manager is a library to standardize error and info logging for all Fabio's projects.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fabio\\UltraLogManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fabio\\UltraLogManager\\Providers\\UltraLogManagerServiceProvider"
            ]
        },
        "aliases": {
            "UltraLog": "Fabio\\UltraLogManager\\Facades\\UltraLog"
        }
    },
    "authors": [
        {
            "name": "fabio cherici",
            "email": "fabio.cherici@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^12.0",
        "illuminate/support": "*",
        "monolog/monolog": "^3.7",
        "fabiocherici/ultra-config-manager": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AutobookNft/UltraConfigManager"
        }
    ]
}